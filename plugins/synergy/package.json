    "backstage",
    "plugin",
    "frontend",
    "backstage.io"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "prepublishOnly": "yarn tsc && yarn build",
  "homepage": "https://github.com/jiteshy/backstage-plugin-synergy",
  "bugs": {
    "url": "https://github.com/jiteshy/backstage-plugin-synergy/issues"
  },
